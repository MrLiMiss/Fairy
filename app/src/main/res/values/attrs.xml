<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActionBarView">
        <attr name="titleStr" format="string" />
        <attr name="rightStr" format="string" />
        <attr name="hideSearch" format="boolean" />
        <attr name="hideLine" format="boolean" />
        <attr name="hideBack" format="boolean" />
        <attr name="rightStrColor" format="color" />
        <attr name="rightStrSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ItemView">
        <attr name="showImg" format="boolean"/>
        <attr name="showRight" format="boolean"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MyView">
        <attr name="my_text" format="string"/>
        <attr name="my_textColor" format="color"/>
        <attr name="my_textSize" format="dimension"/>
    </declare-styleable>

</resources>